{
  "extends": "nx/presets/npm.json",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build"]
      }
    }
  },
  "targetDefaults": {
    "generate": {
      "dependsOn": ["^generate"]
    },
    "build": {
      "dependsOn": ["generate", "^build"]
    },
    "dev": {
      "dependsOn": ["^build"]
    }
  },
  "affected": {
    "defaultBase": "master"
  },
  "plugins": ["@nxrs/cargo"]
}
