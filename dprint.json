{
  "exec": {
    "associations": "**/*.{rs}",
    "rustfmt": "rustfmt --edition 2021",
    "rustfmt.associations": "**/*.rs"
  },
  "json": {},
  "markdown": {},
  "typescript": {
    "quoteStyle": "preferSingle",
    "jsx.quoteStyle": "preferDouble",
    "lineWidth": 120
  },
  "toml": {},
  "dockerfile": {},
  "includes": ["**/*.{toml,dockerfile,rs,json,md,ts,tsx,html}"],
  "excludes": [
    "**/*-lock.json",
    "**/target/",
    "**/build/",
    "**/dist/",
    "**/.next/",
    "**/compiled/",
    "packages/logo/src/",
    "packages/icons/src/"
  ],
  "plugins": [
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/dockerfile-0.2.3.wasm",
    "https://plugins.dprint.dev/exec-0.3.1.json@9351b67ec7a6b58a69201c2834cba38cb3d191080aefc6422fb1320f03c8fc4d",
    "https://plugins.dprint.dev/json-0.15.5.wasm",
    "https://plugins.dprint.dev/markdown-0.14.0.wasm",
    "https://plugins.dprint.dev/typescript-0.71.2.wasm"
  ]
}
